Google検索
javascript:(function(){Q=''+(window.getSelection?window.getSelection():document.getSelection?document.getSelection():document.selection.createRange().text);if(!Q)Q=prompt("Search Keyword?","");if(Q)open('http://www.google.co.jp/search?hl=ja&lr=lang_ja&q='+encodeURIComponent(Q),'_blank');})()

