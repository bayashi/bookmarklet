エキサイト英日翻訳
javascript:(function(){Q=''+(window.getSelection?window.getSelection():document.getSelection?document.getSelection():document.selection.createRange().text);if(!Q)Q=prompt("What's%20Word?","");u='http://www.excite.co.jp/world/english/';if(Q){open(u+'?wb_lp=ENJA&ie=UTF-8&before='+encodeURIComponent(Q),'_blank');}else{location.href=u+'web/?wb_lp=ENJA&wb_url='+encodeURIComponent(location.href);}})()


