今いるサイト内検索
javascript:(function(){Q=''+(window.getSelection?window.getSelection():document.getSelection?document.getSelection():document.selection.createRange().text);if(!Q){Q=prompt("Keyword","");};Q="site:http://"+location.host+"/%20"+Q;if(Q)open('http://www.google.co.jp/search?hl=ja&lr=lang_ja&q='+encodeURIComponent(Q),'_blank');})();


